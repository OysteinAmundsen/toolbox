{
  "name": "@toolbox-web/grid-react",
  "version": "0.13.1",
  "description": "React adapter for @toolbox-web/grid data grid component",
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "default": "./index.js"
    },
    "./features": {
      "types": "./features/index.d.ts",
      "import": "./features/index.js",
      "default": "./features/index.js"
    },
    "./features/*": {
      "types": "./features/*.d.ts",
      "import": "./features/*.js",
      "default": "./features/*.js"
    }
  },
  "keywords": [
    "react",
    "data-grid",
    "web-component",
    "toolbox-web"
  ],
  "license": "MIT",
  "author": "Oystein Amundsen",
  "repository": {
    "type": "git",
    "url": "https://github.com/OysteinAmundsen/toolbox.git",
    "directory": "libs/grid-react"
  },
  "homepage": "https://toolboxjs.com",
  "bugs": {
    "url": "https://github.com/OysteinAmundsen/toolbox/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "devDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@toolbox-web/grid": ">=0.4.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "@toolbox-web/grid": ">=0.4.0"
  }
}
