{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/public.ts",
    "src/lib/plugins/clipboard/index.ts",
    "src/lib/plugins/column-virtualization/index.ts",
    "src/lib/plugins/context-menu/index.ts",
    "src/lib/plugins/editing/index.ts",
    "src/lib/plugins/export/index.ts",
    "src/lib/plugins/filtering/index.ts",
    "src/lib/plugins/grouping-columns/index.ts",
    "src/lib/plugins/grouping-rows/index.ts",
    "src/lib/plugins/master-detail/index.ts",
    "src/lib/plugins/multi-sort/index.ts",
    "src/lib/plugins/pinned-columns/index.ts",
    "src/lib/plugins/pinned-rows/index.ts",
    "src/lib/plugins/pivot/index.ts",
    "src/lib/plugins/reorder/index.ts",
    "src/lib/plugins/selection/index.ts",
    "src/lib/plugins/server-side/index.ts",
    "src/lib/plugins/tree/index.ts",
    "src/lib/plugins/undo-redo/index.ts",
    "src/lib/plugins/visibility/index.ts"
  ],
  "tsconfig": "tsconfig.lib.json",
  "json": "docs/api-generated/api.json",
  "readme": "none",
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": false,
  "excludeNotDocumented": false,
  "excludeExternals": true,
  "externalPattern": ["**/node_modules/**"],
  "sort": ["source-order"],
  "categoryOrder": ["Factory Functions", "Core Types", "Plugin Development", "Events", "*"],
  "blockTags": [
    "@category",
    "@deprecated",
    "@example",
    "@fires",
    "@internal",
    "@param",
    "@remarks",
    "@returns",
    "@see",
    "@throws"
  ]
